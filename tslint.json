{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "no-var-requires": false,
    "semicolon": [
      true,
      "never"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "interface-over-type-literal": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "no-empty-interface": false,
    "no-unused-variable": true
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
