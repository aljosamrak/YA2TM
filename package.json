{
  "name": "yatm",
  "version": "0.1.0",
  "description": "Tab manager",
  "keywords": [
    "typescript",
    "react",
    "chrome-extension"
  ],
  "bugs": {
    "url": "https://github.com/aljosamrak/YATM/issues"
  },
  "author": "Aljosa Mrak <aljosa.mrak@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:aljosamrak/YATM.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "build": "webpack --config webpack.config.ts",
    "start": "webpack --watch",
    "watch": "cross-env NODE_ENV=development webpack -w",
    "build-dev": "cross-env NODE_ENV=development webpack",
    "build-prod": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf ./build",
    "lint": "tslint -p tsconfig.json"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.(ts|tsx|js|jsx)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|tsx|js|jsx)"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/file-transform.ts",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "chart.js": "3.6.0",
    "chartjs-adapter-moment": "^1.0.0",
    "chartjs-react": "3.3.2",
    "moment": "^2.29.1",
    "ng2-charts": "3.0.0-rc.7",
    "react": "17.0.1",
    "react-chartjs-2": "3.3.0",
    "react-dom": "17.0.1",
    "react-ga": "^3.3.0",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/chrome": "0.0.162",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/jest": "27.0.0",
    "@types/jquery": "^3.2.7",
    "@types/node": "^8.10.66",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^10.0.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^6.5.0",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.0-next.91",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^2.0.6",
    "prettier": "^2.4.1",
    "react-scripts": "^5.0.0-next.47",
    "rimraf": "^2.6.1",
    "sass-loader": "^12.3.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^3.3.0",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "tsutils": "^2.6.1",
    "typescript": "^4.4.4",
    "webpack": "^5.62.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  }
}
