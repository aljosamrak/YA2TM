{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react",
    "target": "es5",
    "outDir": "dist/tsc_output",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "noEmitOnError": true,
    "esModuleInterop": true
  },
  "include": [
    "src",
    "webpack.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}